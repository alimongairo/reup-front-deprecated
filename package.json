{
  "name": "reup-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "pre-deploy": "next build && next export",
    "deploy": "npm run pre-deploy && node ./scripts/deploy.js",
    "start": "next start",
    "prepare": "husky install",
    "precommit": "npm run lint:write && npm run prettier && git add -A .",
    "prepush": "npm run lint",
    "prettier": "prettier --write ./pages ./components ",
    "lint": "eslint --debug ./pages ./components",
    "lint:write": "eslint --debug ./pages ./components --fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "antd": "5.0.3",
    "axios": "^1.1.3",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.2",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "next": "13.0.2",
    "next-redux-wrapper": "^8.0.0",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-fast-marquee": "^1.3.5",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.2",
    "sass": "^1.56.0",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "ftp-deploy": "^2.4.3",
    "husky": "^8.0.2",
    "lint-fix": "^0.24.0"
  }
}
